<h2>Stylists</h2>
<h4>Click a name to see their details</h4>

  @if (@Model.Count == 0)
  {
    <p>No stylists to display.</p>
  }
  <ul>
    @foreach (var stylist in Model)
    {
      <li><a href="/stylist/@stylist.GetId()">@stylist.GetName()</a></li>
    }
  </ul>

<p><a href="/stylist/add">Add a stylist</a></p>

<p><a href="/client/add">Add a client</a></p>
